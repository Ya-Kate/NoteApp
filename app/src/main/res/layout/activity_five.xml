<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="@dimen/width"
    android:layout_height="@dimen/height"
    tools:context=".ActivityFive">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/bubbles05" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/skip_activityFive"
        android:layout_width="@dimen/_48dp"
        android:layout_height="@dimen/_48dp"
        android:layout_marginStart="@dimen/_314dp"
        android:layout_marginTop="@dimen/_10dp"
        android:background="@null"
        android:fontFamily="@font/public_sans_medium"
        android:text="@string/skip"
        android:textAllCaps="false"
        android:textSize="@dimen/_18sp" />

    <TextView
        android:id="@+id/spiice_activityFive"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_144dp"
        android:layout_marginTop="@dimen/_48dp"
        android:fontFamily="@font/red_hat_display_medium"
        android:padding="@dimen/_8dp"
        android:text="@string/spiice"
        android:textColor="@color/spiice"
        android:textSize="@dimen/_32sp" />

    <ImageView
        android:layout_width="@dimen/_209dp"
        android:layout_height="@dimen/_432dp"
        android:layout_marginStart="@dimen/_83dp"
        android:layout_marginTop="@dimen/_138dp"
        android:src="@drawable/illustration05" />

    <TextView
        android:id="@+id/work_hard"
        android:layout_width="@dimen/_196dp"
        android:layout_height="@dimen/_29dp"
        android:layout_marginStart="@dimen/_89dp"
        android:layout_marginTop="@dimen/_664dp"
        android:fontFamily="@font/public_sans_regular"
        android:maxLines="1"
        android:text="@string/work_hard_and_level_up"
        android:textAlignment="center"
        android:textColor="@color/spiice"
        android:textSize="@dimen/_18sp" />

    <ImageView
        android:layout_width="@dimen/_52dp"
        android:layout_height="@dimen/_7dp"
        android:layout_marginStart="@dimen/_161dp"
        android:layout_marginTop="@dimen/_733dp"
        android:src="@drawable/diaporama04" />

</FrameLayout>