<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="@dimen/width"
    android:layout_height="@dimen/height"
    tools:context=".ActivitySix">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/bubbles06" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/skip_activitySix"
        android:layout_width="@dimen/_48dp"
        android:layout_height="@dimen/_48dp"
        android:layout_marginStart="@dimen/_312dp"
        android:layout_marginTop="@dimen/_10dp"
        android:background="@null"
        android:fontFamily="@font/public_sans_medium"
        android:text="@string/skip"
        android:textAllCaps="false"
        android:textSize="@dimen/_18sp" />

    <TextView
        android:id="@+id/spiice_activitySix"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_144dp"
        android:layout_marginTop="@dimen/_48dp"
        android:fontFamily="@font/red_hat_display_medium"
        android:padding="@dimen/_8dp"
        android:text="@string/spiice"
        android:textColor="@color/spiice"
        android:textSize="@dimen/_32sp" />

    <ImageView
        android:layout_width="@dimen/_311dp"
        android:layout_height="@dimen/_311dp"
        android:layout_marginStart="@dimen/_32sp"
        android:layout_marginTop="@dimen/_211dp"
        android:scaleType="center"
        android:src="@drawable/illustration06" />

    <TextView
        android:id="@+id/enjoy_your_progess"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/_29dp"
        android:layout_marginStart="106dp"
        android:layout_marginTop="@dimen/_664dp"
        android:fontFamily="@font/public_sans_regular"
        android:maxLines="1"
        android:text="@string/enjoy_your_progess"
        android:textColor="@color/spiice"
        android:textSize="@dimen/_18sp" />

    <ImageView
        android:layout_width="@dimen/_52dp"
        android:layout_height="@dimen/_7dp"
        android:layout_marginStart="@dimen/_161dp"
        android:layout_marginTop="@dimen/_733dp"
        android:src="@drawable/diaporama05" />

</FrameLayout>